						<xsa:sequence>
							<xsa:element name="targy" type="xsa:string" />
							<xsa:element name="idopont">
								<xsa:complexType>
									<xsa:sequence>
										<xsa:element name="nap" type="xsa:string" />
										<xsa:element name="tol" type="xsa:positiveInteger" />
										<xsa:element name="ig" type="xsa:positiveInteger" />
									</xsa:sequence>
								</xsa:complexType>
							</xsa:element>

							<xsa:element name="helyszin" type="xsa:string" />
							<xsa:element name="oktato" type="xsa:string" />
							<xsa:element name="szak" type="xsa:string" />
						</xsa:sequence>
						<xsa:attribute name="id" type="xsa:ID" />
						<xsa:attribute name="tipus" type="xsa:string" default="előadás" />
					</xsa:complexType>
				</xsa:element>
			</xsa:sequence>
		</xsa:complexType>
